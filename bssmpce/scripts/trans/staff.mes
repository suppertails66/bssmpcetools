        // size
        [
//          pushInt         $A0
//          pushShort       $10
//          mul
          pushArrayWord $4
          pushInt 128
          mul
          retVar          
        ]
        
        // dst
        [
          pushLong        $6000
          retVar          
        ]
      )
      
      // advance src past graphics data
      writeValueWord $3
      [
        pushArrayWord $3
        
        pushArrayWord $4
        pushInt 128
        mul
        
        add
        
        retVar
      ]
      
      // send sprite data to vram
      op19            
      (
        [
          pushImmed       $4
          retVar          
        ]
        
        // src
        [
