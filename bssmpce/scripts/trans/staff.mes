//          mul
          pushArrayWord $4
          pushInt 8
          mul
          retVar          
        ]
        
        // dst
        [
          pushLong        $0F00
          retVar          
        ]
      )
      
      // advance src past sprite data
      writeValueWord $3
      [
        pushArrayWord $3
        
        pushArrayWord $4
        pushInt 8
        mul
        
        add
        
        retVar
      ]
    
      ret             
    }
